<% if (productos.length > 0) { %> 
    <table class="table mt-3" style="width: 35vw;">
        <thead style="color: rgb(51, 40, 40); border-bottom:1px solid blue;">
            <tr>
                <th scope="col">id</th>
                <th scope="col">Producto</th>
                <th scope="col">Foto</th>
                <th scope="col">Precio</th>
            </tr>
        </thead>
        <tbody>
            <% for(const producto of productos) { %>
                <tr>
                    <td><%= producto.id %></td>
                    <td><%= producto.title %></td>
                    <td>
                        <img src="<%= producto.thumbnail %>" alt="<%= producto.title %>" style="width: 70px;">
                    </td>
                    <td><%= producto.price %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
<% } else { %>
    <div class="container mt-3">
        <h3 class="alert alert-danger">No encontrado</h3>
    </div>
<% } %> 